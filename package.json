{
  "name": "exta",
  "version": "0.0.1",
  "description": "File parser",
  "main": "dist/index.js",
  "author": {
    "name": "Radostin Romanov",
    "url": "https://github.com/radromanov"
  },
  "license": "MIT",
  "bin": {
    "parser": "./dist/index.js"
  },
  "scripts": {
    "chmod": "chmod +x ./dist/index.js",
    "unlink:cli": "pnpm uninstall --global parser",
    "link:cli": "(pnpm unlink:cli || true) && pnpm chmod && tsconfig-paths/register && pnpm link --global",
    "dev": "tsc -w"
  },
  "dependencies": {
    "commander": "^12.1.0",
    "tsconfig-paths": "^4.2.0",
    "winston": "^3.13.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.9",
    "jest": "^29.7.0",
    "module-alias": "^2.2.3",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.2"
  },
  "_moduleAliases": {
    "@": "dist",
    "@shared": "dist/shared"
  }
}
